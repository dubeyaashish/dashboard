{
    "name": "job-management-dashboard",
    "version": "1.0.0",
    "description": "Advanced job management dashboard with React and Node.js",
    "type": "module",
    "main": "server.js",
    "scripts": {
      "start": "node server.js",
      "dev": "nodemon server.js",
      "client": "cd client && npm start",
      "build": "cd client && npm run build",
      "dev:full": "concurrently \"npm run dev\" \"npm run client\""
    },
    "dependencies": {
      "cors": "^2.8.5",
      "dotenv": "^16.3.1",
      "express": "^4.18.2",
      "mongoose": "^7.5.0",
      "node-cron": "^3.0.2"
    },
    "devDependencies": {
      "concurrently": "^8.2.1",
      "nodemon": "^3.0.1"
    },
    "engines": {
      "node": ">=16.0.0"
    }
  }